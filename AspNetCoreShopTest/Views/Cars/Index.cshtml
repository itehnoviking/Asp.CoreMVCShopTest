@using AspNetCoreShopTest.Models
@model IEnumerable<Car>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
    <head>
        <meta name="viewport" content="width=device-width" />
    </head>
    <body>
        <h2>All cars</h2>
        @{
            foreach(var car in Model){
                <div>
                    <h2>Model: @car.Name</h2>
                    <p>Price: @car.Price.ToString("c")</p>
                </div>
            }
        }
    </body>
</html>
